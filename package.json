{
  "name": "quick-cmd",
  "version": "2.0.0",
  "description": "A Firefox webextension that tries to mimic vscode's quick commands popup. Open with Alt-Shift-P",
  "main": "",
  "scripts": {
    "pack": "web-ext build --overwrite-dest",
    "sign": "web-ext sign",
    "build:watch": "webpack -w",
    "build:prod": "webpack --mode production && npm run pack",
    "start": "web-ext run --source-dir ./ --firefox-profile=\"%APPDATA%\\Mozilla\\Firefox\\Profiles\\emtl8nfs.web-ext-test\" --keep-profile-changes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victal/quickcommands.git"
  },
  "author": "UliPlabst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victal/quickcommands/issues"
  },
  "homepage": "https://github.com/victal/quickcommands#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^5.2.7",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^6.0.1",
    "sass-loader": "^10.2.0",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.4.4",
    "web-ext": "^5.3.0",
    "web-ext-types": "^3.2.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.0",
    "ts-loader": "^8.3.0"
  },
  "dependencies": {
  }
}
